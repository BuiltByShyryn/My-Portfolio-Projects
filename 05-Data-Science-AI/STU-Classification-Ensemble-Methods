    "joblib.dump(best_model, MODEL_PATH)\n",
    "print(\"Saved:\", MODEL_PATH)\n",
    "\n",
    "loaded = joblib.load(MODEL_PATH)\n",
    "print(\"Loaded type:\", type(loaded))\n",
    "\n",
