   ]
  },
  {
   "cell_type": "code",
   "execution_count": 30,
   "id": "7ba30661",
   "metadata": {
    "id": "7ba30661"
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "TEST F1: 0.9537037037037037\n",
      "Loaded OK: <class 'sklearn.ensemble._forest.RandomForestClassifier'>\n"
     ]
    }
   ],
   "source": [
    "import joblib\n",
    "from sklearn.metrics import f1_score\n",
    "\n",
    "best = gs.best_estimator_\n",
    "best.fit(X_train, y_train)\n",
    "\n",
    "pred = best.predict(X_test)\n",
    "print(\"TEST F1:\", f1_score(y_test, pred))\n",
    "\n",
    "joblib.dump(best, \"rf_best.joblib\")\n",
    "loaded = joblib.load(\"rf_best.joblib\")\n",
    "print(\"Loaded OK:\", type(loaded))"
   ]
